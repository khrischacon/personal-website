                            text-transform:uppercase !important;
                            border:solid 1px #cb4f00 !important;
                            -moz-box-shadow: #B2B2B2 0 1px 5px 0;
                            -webkit-box-shadow: #B2B2B2 0 1px 5px 0;
                            box-shadow: #B2B2B2 0 1px 5px 0;
                        }
                        a.save:hover { @include linear-gradient(#fd7a10, #f1670f);}
                    }
                }

                section#alerts fieldset.data .field, section#onlinePayments fieldset.data .field { width:380px!important; }

                section#alerts {

                    select { 

                        @include inline-block; width:90px!important; height:20px!important; margin:0 5px!important; font-size:0.875em!important; border:solid 1px #dedede!important; }

                    label { 

                        @include inline-block; width:214px!important; color:$medium_gray!important;

                        em { font-weight:bold!important; display:inline-block; width:214px!important; }

                    }

                    input[type="checkbox"] { @include inline-block; width:auto!important; border:0; text-transform:none; }

                    .field.fix { 

                        label { @include inline-block; width:318px!important; }
                        
                    }

                    .description { margin-top:10px!important; }

                    .data {

                        header {

                            width:444px; height:25px!important; margin:0!important;

                            p { display:inline-block; width:50px!important; float:left; }

                            p:first-child { margin-right:198px!important; width:100px!important; }

                        }

                        label { color:#4c4c4c!important; }

                        .field.checkbox { 

                            label { width:auto!important; font-size:1em !important; text-transform:none !important; }
                            input[type="checkbox"] { margin-top:-2px; vertical-align:bottom; }

                        }

                    }

                }
                section#paperless {

                    label { width:290px!important; font-size:0.9em!important; text-transform:none!important;  }

                    .data { display:inline-block!important; float:left!important; }
                }

                section#onlinePayments {

                    .top { @include inline-block; 

                        label, input { width:auto; display:inline!important; }

                        label { font-size:0.9em!important; }

                        h4 { font-family:Arial, Helvetica, sans-serif; margin-bottom:7px!important; }

                    }

                    .data {

                        label { text-transform:uppercase!important; }

                        img { margin-bottom:15px!important; }

                        .field.checkbox {

                            label { text-transform:none!important;}

                        }

                    }

                }
            }
        }
    }

