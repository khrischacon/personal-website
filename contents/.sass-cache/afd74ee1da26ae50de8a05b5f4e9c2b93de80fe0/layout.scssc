    text-decoration:underline;

}

fieldset .field.checkbox { 

    label { font-size:12px }

}

#profile, #security, #preferences { 

    width: 63%; padding-left: 37%;

    .field { width: 560px;}

}

#preferences {

    section header {cursor: pointer;}

    fieldset {display: none;}

    section.opened { background:url(../images/ao_registration_preferences_active_preference_bg.png) repeat }

}

#profile_widget {

    position: absolute; top: 0; left: 1px; display: none; background: #fff; min-height: 500px;

    h4.security {color: $dark-orange;}

    h4.profile {color:$yellow;}

    h4.preferences {color: $blue-gray;}

    .profile-box { 

        @include inline-block; font-weight:normal; margin-bottom:25px; color:$medium_gray; width:260px; height:auto; @include linear-gradient(#f9f9f9, #f7f7f7); @include round-corner; border:1px solid #dedede;

        h3 { font-size:1.5em; padding:15px 10px 0; } 
        
        p { 

            font-size:0.85em; padding:10px 10px 20px; text-transform:uppercase; 

            span { margin-left:10px; }
            
        }

        a { @include inline-block; text-transform:uppercase; font-size:0.95em; width:240px; height:25px; padding:15px 10px 10px; background:url(../images/ao_registration_profile_box_arrow_down.png) no-repeat 230px center #eeeeee; @include round-corner-bottom; @include inner-shadow-top }
    
    }

    .promo-box { 

        @include inline-block; width:250px; @include inline-block; text-align:left; vertical-align:middle;
        
        .js & {display: none;}

        h4 { @include inline-block; width: 180px; font-size:1.1em; margin-bottom:5px; }
        
        p { @include inline-block; width: 180px; font-size:0.875em; line-height:1.25em; } 
        
        img { @include inline-block; float: left; margin-right: 10px; }

    }

}

.cover {

    position: absolute; @include corner-to-corner; background: #fff url(../images/wait.gif) 50% 250px no-repeat; display: none;

}

