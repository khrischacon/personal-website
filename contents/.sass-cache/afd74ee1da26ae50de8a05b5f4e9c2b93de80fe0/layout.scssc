    nav a.complete { width:505px; background:red; }


}
//================================ main footer:

footer {
    
    @include inline-block; width: 855px; padding: 0 40px; background: #fff; line-height:24px; font-size:0.75em; font-weight:normal; color:$medium_gray; text-align:justify; 
    
    > * { width:auto; @include inline-block; vertical-align:middle; }
    
    nav { text-align:left; margin-left:-40px }
    
    nav a { color:$dark-orange; text-decoration:none; }
    
    nav a:first-child { margin-right:5px; }
    
    nav a:last-child { margin-left:5px; }
    
    p { display:block; margin-left:-40px; }
    
    img { float:right }

}

body hr {
    border:0;
    color: #e0e0e0;
    background-color: #e0e0e0;
    height:1px;
}
//================================ forms


form {
    
    header {
        
        position: relative;
        
        h2 {
            text-align: left;
            width: 100%;
            padding-bottom: 30px; 
        }
        
        aside {
            position: absolute;
            right: 0;
            top: 0;
            white-space: nowrap;
        }
    }
    form {
        padding: 5px 0;
        display: block;
        text-align: center;
    }
}

.fieldset_errors {
    color: $red;
    display:block;
    width:200px;
    text-align:right;
    background:#666666;
    padding:10px;
}

.field {
    @include inline-block;
    width: 100%;
    padding: 10px 0 0;
    position: relative;
    &.required {

        label:before {
            color: #aaa;
        }
        &.completed label:before {
            color: green;
            content:url(../images/ao_registration_form_verified_check.png);
        }
        &.completed .tool_tip .mask { display:none; }
    }   
    label {
        width: 100%;
        display: block;
        margin-bottom: 3px; 
        white-space: nowrap;
        color:$gray;
        font-weight:normal;
        text-transform:uppercase;
        font-size:0.75em;
    }
    label:before {
        @include inline-block;
        content: "";
        background: url(../images/ao_registration_form_validation_check.png) 0 0 no-repeat;       
        width: 22px;
        height: 14px;
    }
    &.checkbox {
        label, input {
            @include inline-block;
        }
        label {
            white-space: normal;
            width:auto;
            text-transform:inherit;
            font-size:0.875em;

            &:before {
                content: none;
            }
        }
        input { width:auto; }
    }

    &.field-with-errors {

        label{color:$red;}

        label:before{background-image: url(../images/ao_registration_form_error_check.png);}

        input {

            border:solid 2px $red;

            &:focus{border-color: #999; outline-color: $red;}

        }
        &.tool_tip .mask { display:inline-block; }
    }

    &.select_field { margin-top: -8px; }
    
    input {
        border: 2px solid #dedede;
        @include round-corner;
        padding: 7px 1%;
        width: 75%;
        margin-bottom: 10px;
        font-size: 1.2em;
        line-height: 1.2em;
        font-weight: normal;
        color: #666;
        @include inner-shadow;
        font-size:0.875em;
    }

    .tool_tip .mask { display:inline-block; }

    input:focus { 

        border:solid 2px #666666;

    }

    .tool_tip {
        position:absolute;
        top:-23px;
        right:-7px;
        z-index:10;
        
        .mask { width:16px; height:146px; background:#f2f2f2; position:absolute; top:2px; left:20px; }
    }
    
    input[type='submit']{
        @include linear-gradient(#f1670f, #e15800);
        display:block;
        width:140px;
        height:48px;
        @include round-corner;
        color:$white;
        text-transform:uppercase;
        cursor:pointer;
        border:solid 1px #cb4f00;
        -moz-box-shadow: #B2B2B2 0 1px 5px 0;
        -webkit-box-shadow: #B2B2B2 0 1px 5px 0;
        box-shadow: #B2B2B2 0 1px 5px 0;
        font-size:1.2em;
    }
    
    input:hover[type='submit'] {
        @include linear-gradient(#fd7a10, #f1670f);
    }
    
    select {
        border: 1px #333 solid;
        border-radius: 5px;
        width: 90%;
        color: #666;
        font-weight: 900;
        font-size: 1.2em;
        line-height: 1.2em;
        height: 40px;
        padding-top: 3px;
    }
    a.tip { // the tooltip icon:
        .js & {
            display: block;
            padding-left: 26px;
            height: 50px;
            position: absolute;
            left: 96%;
            top: 34px;
            background: url(../../images/tip.png) 0 0 no-repeat;
            overflow: hidden;       
            width: 0;
            &:focus {
            background-position: -50px 0;
            }
        }
        &.active {
            background-position: -100px 0;
            &:focus {
                background-position: -150px 0;
            }
            &:after {
                content: 'â–²';//&#9650;
                position: absolute;
                display: block;
                bottom: -5px;
                left: 0;
                color: #666;
                font-size: 1.6em;
                transform:scale(2,1); /* W3C */
            -webkit-transform:scale(2,1); /* Safari and Chrome */
            -moz-transform:scale(2,1); /* Firefox */
            -ms-transform:scale(2,1); /* IE 9 */
            -o-transform:scale(2,1); /* Opera */
            }
        }
    }
}
.field.digits {

    width:41%; float:left;

    input { width:85%; @include inline-block; height:auto;}

}


