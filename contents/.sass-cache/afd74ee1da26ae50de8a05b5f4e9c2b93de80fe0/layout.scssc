//================================ global bits:

body {
    background:url(../images/ao_registration_background.png) repeat;
    text-align: center; 
    font-family:'helveticaNeu', Arial, Helvetica, sans-serif;
    font-size:1em;
    color:$gray;
    > * {
        width: 980px;
        margin: 0 auto;
        text-align: left;
    }
}


h1 { font-size: 2em; }

h2 { font-size: 1.5em; }

h3 { font-size: 1.25em; }

h4 { font-size: 0.85em; }

p, a, label { font-family: Arial, Helvetica, sans-serif; }

h1, h2, h3, h4, p { padding-top:2px }

a { outline:none; color:#e15800; text-decoration:none; cursor:pointer; }

input { outline:none; }

select { border:solid 2px #dedede; outline:none; }

select:focus { border:solid 2px #666666; }

//================================ main header:

body > header {
    
    background: #444; padding: 10px 31px 31px 41px; width:908px; height:119px; font-weight:normal;
    
    h1 { font-size:2.69em; color:$white; display:inline-block; font-weight:normal; margin-left:38px; }
    
    img { margin-top:14px; }
    
    nav.login { 

        display:block; width:100%; height:25px; text-align:right; font-size:0.875em; margin-top:0; 
        
        a { display:inline-block; padding-top:4px; height:25px; color:$white; margin-right:18px; margin-top:5px;  }
        
        a:last-child { margin-right:0; background:url(../images/ao_registration_safe_secure_icon.png) no-repeat 0 0; padding-left:17px; }

    }

}

//================================ Step Navigation:

body > nav {
    
    display:block; background:#808080; border-top:solid 1px #ffffff; width:980px; height:38px; padding:0; font-size:0.85em;
    
    a { @include inline-block; font-weight:normal; cursor: default; color:$light-gray; background:#808080; text-decoration:none; padding:10px 40px 10px 38px; width:auto; height:18px; border-right:solid 1px $white; @include transition((padding-right 1s, background 1s));}
    
    a:last-child { border:none }
    
    a.active { background:#e15800; color:$white; width:auto; padding-right:44.593%%; }
    a.complete { background:#e15800; color:$white; width:505px; padding-right:44.593%%; }
}

//================================ main section

body > section.main {
    
    @include inline-block; background:#fff; position: relative; width:900px; padding:50px 40px; text-align:justify;

    footer nav a:hover { text-decoration:underline; }

}

form > fieldset > {

     article, aside { @include inline-block; }
 
}

form > fieldset#verify > {

     article { 
        
        width:50%;

        label:before { content:none; }

        #ssn {

            .tool_tip {
                top:5px;
                left:167px;
                z-index:10;
                
                img {  }
            }
        }
     }

     aside {width: 49%;}   
}

form > fieldset#profile >, form > fieldset#security >, form > fieldset#preferences > {
     
     article {width:90%;}

}

form > fieldset#profile > { 

    article { 

        height: auto;

        input {width: 45%;}
        
        input[type="submit"] {width: 140px; font-size: 1.2em;}

    }
}
form > fieldset#security > { 

    article { 

        @include inline-block; text-align:justify; vertical-align:middle;  

        h1 { margin-bottom:25px }

        .field {
            &.required {

                label{background: $dark-gray;}

                label:before {content: none;}    

                &.completed label{background: $light-green; }

                &.completed label:before {content: none;}

            } 

            label { @include inline-block; font-size:1.5em; width:42px; height:33px; padding-top:9px; color:$white; background:$gray; @include circle; vertical-align:middle; text-align:center; }
        
            select { @include inline-block; margin-left:10px; margin-right:10px; border:solid 2px #DEDEDE; height:34px; }
            
            select:focus { border:solid 2px #666666; }

            select, input { width:40%; font-size:0.875em; }
        
            input[type="submit"] { width:140px; font-size:1.2em; margin-top:15px; }

        }       
    }
}


form > fieldset#preferences { 
    
    font-size:0.875em;

    aside {  
       .promo-box {  
            h4 { color:$blue-gray; }
       } 
    }

    article { 
        
        @include inline-block;
        
        h1 { margin-left:14px; }

        p {  width:400px; line-height:1.25em; }

        p.sectionDesc { margin-left:14px; margin-bottom:25px; }
        
        hr { width:519px; margin:5px 0 0;}

        section {
    
            margin-top:0; margin-bottom:10px; width:87%; height:auto; @include inline-block; text-align:left; position:relative; vertical-align:middle; padding-top:25px; padding-left:75px; padding-bottom:35px;

            figure {
                
                position:absolute; top: 25px; left:11px;
            
            }

            header {

                h2 { padding-bottom:1px }

                p { margin-bottom:3px; }

                button { position:absolute; width:82px; height:32px; top:0; right:17px; border:none; text-indent:-9999999px; cursor:pointer;background:url(../images/ao_registration_preferences_add_button.png) no-repeat top; }

                button:hover { background-position:bottom }

                a.trigger { text-transform:uppercase; font-size:0.875em; }

            }

            &.edited header button {background:url(../images/ao_registration_preferences_edit_button.png) no-repeat top;}

            &.edited header button:hover { background-position:bottom; }

            .data {

                background:#f2f2f2; @include round-corner; border:1px solid #dedede; width:380px; height:auto; padding:20px; margin:20px 0 20px 2px;

                header {

                    > * { width:auto; @include inline-block; @include justified }

                }

                label { white-space:normal; text-transform:inherit;}

                label:before { content:none; }
                                
                input[type="text"] { width:70%; }

                hr { width:100%; }

            }
            .field.action {

                width:435px; min-height:27px; text-align: right;

                a { text-transform:uppercase; text-align:center; margin-right:10px; color:$gray; }

                a.save, a.remove { height:auto; @include round-corner; font-size:1em; cursor:pointer; padding:8px 16px; z-index:10; }
                a.remove{
                    @include linear-gradient(#ffffff, #f7f7f7);
                    @include round-corner;
                    color:$dark-gray;
                    width:200px; 
                    text-transform:capitalize;
                    border:solid 1px #dedede;
                    -moz-box-shadow: #B2B2B2 0 1px 5px 0;
                    -webkit-box-shadow: #B2B2B2 0 1px 5px 0;
                    box-shadow: #B2B2B2 0 1px 5px 0;
                    margin-right:79px;
                }

                a.save{
                    @include linear-gradient(#f1670f, #e15800);
                    color:$white;
                    width:100px; 
                    text-transform:uppercase;
                    border:solid 1px #cb4f00;
                    -moz-box-shadow: #B2B2B2 0 1px 5px 0;
                    -webkit-box-shadow: #B2B2B2 0 1px 5px 0;
                    box-shadow: #B2B2B2 0 1px 5px 0;
                }
                a.save:hover { @include linear-gradient(#fd7a10, #f1670f);}
            }
        }

        section#alerts fieldset.data .field, section#onlinePayments fieldset.data .field { width:380px; }

        section#alerts {

            select { 

                @include inline-block; width:90px; height:20px; margin:0 5px; font-size:0.875em; border:solid 1px #dedede; }

            label { 

                @include inline-block; width:214px; color:$medium_gray;

                em { font-weight:bold; @include inline-block; width:100%; }

            }

            input[type="checkbox"] { @include inline-block; width:auto; }

            .field.fix { 

                label { @include inline-block; width:318px; }
                
            }

            .description { margin-top:10px; }

            .data {

                header {

                    height:25px; margin:0;

                    p:first-child { margin-right:218px }

                }

                label { color:#4c4c4c; }

                .field.checkbox { 

                    label { width:auto; }

                }

            }

        }
        section#paperless {

            label { width:290px; font-size:0.9em  }
        }

        section#onlinePayments {

            .top { @include inline-block; 

                label, input { width:auto; display:inline; }

                label { font-size:0.9em }

                h4 { font-family:Arial, Helvetica, sans-serif; margin-bottom:7px }

            }

            .data {

                label { text-transform:uppercase; }

                img { margin-bottom:15px; }

                .field.checkbox {

                    label { text-transform:none; }

                }

            }

        }
    }
}

.js form fieldset#congratulations { 

    min-height:270px; padding:20px 0; background:url(../images/ao_registration_congratulations_image.png) no-repeat 460px 20px;
    
    h1, h3 { line-height:1.25em; }
    
    h3 { @include inline-block; width:400px; }
    
    .field { width:auto; display:block; margin-top:10px; margin-left:17px; }
    
